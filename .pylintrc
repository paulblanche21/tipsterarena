[MASTER]
load-plugins=pylint_django

[MESSAGES CONTROL]
disable=C0111,C0103,C0114,C0115,C0116,C0303,W0511,R0903,R0913,R0914,R0904

[FORMAT]
max-line-length=120

[REPORTS]
output-format=colorized

[DJANGO]
django-settings-module=tipsterarena.settings

[DESIGN]
max-args=8
max-attributes=15
max-branches=15
max-locals=20
max-parents=7
max-public-methods=20
max-returns=6
max-statements=50

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes 